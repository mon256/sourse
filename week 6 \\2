
#include <iostream>
using namespace std;

class Node {
public:
    char data;
    Node* next;
};


int main()
{
    Node*head = NULL;
    Node*second = NULL;
    Node*third = NULL;
    Node*fourth = NULL;
    Node*fifth = NULL;
    Node* six = NULL;
    Node* seventh = NULL;
    Node* eight = NULL;
    Node* nine = NULL;
   


    head = new Node();
    second = new Node();
    third = new Node();
    fourth = new Node();
    fifth = new Node();
    six = new Node();
    seventh = new Node();
    eight = new Node();
    nine = new Node();
    


   
    head->data = ' M';
    cout << head->data<<endl;
    head->next = second;
    second->data = 'e';
    cout << second->data<<endl;
    second->next = third;
    third->data = 'r';
    cout << third->data<<endl;
    head->next = fourth;
    fourth->data = ' n';
    cout << fourth->data<<endl;
    fourth->next = fifth;
    fifth->data = 'a';
    cout << fifth->data<<endl;
    fifth->next = NULL;
    six->data = ' v';
    cout << six->data<<endl;
    six->next = seventh;
    seventh->data = ' e';
    cout << seventh->data;
    seventh->next = eight;
    eight->data = ' r';
    cout << eight->data;
    eight->next = NULL;
    return 0;
}
